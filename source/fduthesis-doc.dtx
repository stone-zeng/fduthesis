% \begin{macro}{\marg,\oarg,\parg}
% 几种命令参数：
% \begin{itemize}
%   \item 必选参数：|{|\meta{arg}|}|；
%   \item 可选参数：|[|\meta{arg}|]|；
%   \item 图形参数：|(|\meta{arg}|)|。
% \end{itemize}
%    \begin{macrocode}
\renewcommand*\marg[1]{\{\meta{#1}\}}
\renewcommand*\oarg[1]{[\meta{#1}]}
\renewcommand*\parg[1]{(\meta{#1})}
%    \end{macrocode}
% \end{macro}
